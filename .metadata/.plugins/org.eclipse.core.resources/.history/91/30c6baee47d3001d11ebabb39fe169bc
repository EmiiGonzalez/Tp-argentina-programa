package modelo;

import lombok.Data;

@Data
public class Pronostico {
	
	Partido partido = new Partido();
	String linea;
	Equipo equipo1; 
	Equipo equipo2; 

	
	public Pronostico() {
	}
	
	public Pronostico(Partido partido, String linea) {
		this.partido = partido;
		this.linea = linea;
		
		 String nombreEquipo1 = linea.split(",")[1];
		 int golesEquipo1 = Integer.parseInt(linea.split(",")[2]);
		 String nombreEquipo2 = linea.split(",")[4];
		 int golesEquipo2 = Integer.parseInt(linea.split(",")[3]);
		 String ronda = linea.split(",")[0];
		    
		 this.equipo1 = new Equipo(nombreEquipo1, golesEquipo1);
		 this.equipo2 = new Equipo(nombreEquipo2, golesEquipo2);
		
	}
}
