package modelo;

import lombok.Data;

@Data
public class Persona {
	private String nombre;
	private int puntaje;
	private int aciertos;
	
	public Persona(String nombre) {
		this.nombre = nombre;
	}

	public Persona() {

	}

	public void setPuntaje(boolean puntaje) {
		if (puntaje) {
			this.setPuntaje() += 1;
		}
	}
	
	
	
}
