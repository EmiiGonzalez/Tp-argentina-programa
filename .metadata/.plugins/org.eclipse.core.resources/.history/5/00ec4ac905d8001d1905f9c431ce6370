package modelo;

import java.util.ArrayList;
import java.util.List;

import lombok.Data;


public class Fase {
	
	private List<Persona> participantesPorFase = new ArrayList<>();
	private String nombreFase;

	public Fase(List<Persona> participantesPorFase) {
		this.participantesPorFase = participantesPorFase;
	}
	
	public Fase() {
	}

	public List<Persona> getParticipantesPorFase() {
		return participantesPorFase;
	}

	public void setParticipantesPorFase(Persona p) {
		this.getParticipantesPorFase().add(p);
	}
	
	
	
}
