package modelo;

import lombok.Data;

@Data
public class Apuesta {
	private String nombre;
	private boolean puntaje;
	private String numeroDeRonda;
	
	public Apuesta(String nombre, String numeroR) {
		this.nombre = nombre;
		this.numeroDeRonda = numeroR;
	}

	public Apuesta() {

	}

	public void setPuntaje(boolean puntaje) {
		this.puntaje = puntaje;	
	}
		
}
